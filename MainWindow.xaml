        <Label Content="Username" HorizontalAlignment="Left" Height="29" Margin="350,105,0,0" VerticalAlignment="Top" Width="98"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="41" Margin="350,206,0,0" VerticalAlignment="Top" Width="128"/>
    </Grid>
</Window>
